{
  "name": "sveltestack",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx src/lib/server/db/seed.ts reset",
    "db:reset-seed": "tsx src/lib/server/db/seed.ts reset-seed",
    "db:seed": "tsx src/lib/server/db/seed.ts seed",
    "db:start": "docker-compose up -d",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.937.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@lucide/svelte": "^0.544.0",
    "@node-rs/argon2": "^2.0.2",
    "@uppy/aws-s3": "^5.0.2",
    "@uppy/core": "^5.1.1",
    "@uppy/svelte": "^5.0.2",
    "better-auth": "^1.4.1",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "formsnap": "^2.0.1",
    "pg": "^8.16.3",
    "pretty-bytes": "^7.1.0",
    "superforms": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "ulid": "^3.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.1",
    "@internationalized/date": "^3.10.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-vercel": "^6.1.2",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@vitest/browser-playwright": "^4.0.13",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "mode-watcher": "^1.1.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "svelte-sonner": "^1.0.6",
    "sveltekit-superforms": "^2.28.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.13",
    "vitest-browser-svelte": "^2.0.1"
  }
}
